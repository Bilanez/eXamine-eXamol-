<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="label" prefHeight="325.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
 
   <children>
      <Label fx:id="eXamol_label" layoutX="43.0" layoutY="41.0" text="eXamol: Settings">
         
         <font>
            <Font size="18.0" />
         </font>
         
      </Label>
      
      <CheckBox fx:id="show_c" layoutX="43.0" layoutY="131.0" mnemonicParsing="false" text="Show Charge" />
      <CheckBox fx:id="show_h" layoutX="35.0" layoutY="92.0" mnemonicParsing="false" text="Show Hydrogen" />
      <Button fx:id="start_button" layoutX="254.0" layoutY="248.0" mnemonicParsing="false" text="Start eXamole" />
      <Button fx:id="choose_button" layoutX="270.0" layoutY="80.0" mnemonicParsing="false" onAction="#handleButtonChoose" text="Choose " />
      <Button fx:id="create_button" layoutX="274.0" layoutY="127.0" mnemonicParsing="false" text="Create" />
      
   </children>
   
</AnchorPane>
